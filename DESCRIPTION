Package: hansen
Title: Datasets and Unofficial Solutions from 'Econometrics' by Bruce E. Hansen
Version: 0.0.0.9000
Authors@R: c(
    person(
        given = "Bruce E.",
        family = "Hansen",
        role = "cph",
        email = "m.sepulveda@mail.utoronto.ca",
        comment = c(ORCID = "0000-0003-1017-7574", "Econometrics textbook")
